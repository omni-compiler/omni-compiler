OMNI_FX2X_MUST_OPT="-cp ${OMNI_JAR4}:${OMNI_JAR3}:${OMNI_JAR2}:${OMNI_JAR1} exc.util.omompx -fxmpf -xf -M${OMNI_HOME}/include -M${OMNI_HOME}/fincludes"
