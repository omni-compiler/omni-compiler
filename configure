  withval=$with_thread; thread_type=$withval
else
  thread_type=""
fi


# Default lock type
# Check whether --with-lock or --without-lock was given.

# Check whether --with-lock was given.
if test "${with_lock+set}" = set; then :
  withval=$with_lock; lock_type=$withval
