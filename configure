    fi

    if ! test -d "$GMP_LIB_DIR"; then
      as_fn_error $? "$GMP_LIB_DIR not found.  Please attach --with-gmp=<DIR> or --with-gmp-lib=<DIR>." "$LINENO" 5
    fi

    # check gmp.h
    tmp_file=/tmp/_omni_$$
    tmp_file_c=/tmp/_omni_$$.c
    echo "#include <gmp.h>" > $tmp_file_c
    ${CC} -E -I$GMP_INCLUDE_DIR $tmp_file_c 2> /dev/null > /dev/null
    if test $? -ne 0; then
	rm -f $tmp_file_c
