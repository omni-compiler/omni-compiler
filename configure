# --------------------------------------------------------------------
# 32/64 bit int type

rm -rf ./szchk ./szchk.*
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: Creating size check program" >&5
$as_echo "Creating size check program" >&6; }
${CC} ${CPPFLAGS} ${CFLAGS} ${hasLLFlags} ./buildutils/szchk.c -o ./szchk
if test ! -x ./szchk; then
    as_fn_error $? "can't create size check program." "$LINENO" 5
fi

F2C_INT64_FLAGS=""
for i in 16 32 64
do
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking $i bit integer" >&5
$as_echo_n "checking $i bit integer... " >&6; }
    typName=`./szchk $i`
    if test "x${typName}" = "xunknown"; then
    case $i in
    16)
        TYPE_INT16="short"

        asump=${TYPE_INT16};;
    32)
        TYPE_INT32="int"

        asump=${TYPE_INT32};;
    64)
        TYPE_INT64="long long int"

        asump=${TYPE_INT64};;
    esac
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: not supported. use $asump" >&5
$as_echo "not supported. use $asump" >&6; }
    else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: $typName" >&5
$as_echo "$typName" >&6; }
    case $i in
    16)
        $as_echo "#define HAS_INT16 1" >>confdefs.h


        TYPE_INT16=${typName}
        ;;
    32)
        $as_echo "#define HAS_INT32 1" >>confdefs.h


        TYPE_INT32=${typName}
        ;;
    64)
        $as_echo "#define HAS_INT64 1" >>confdefs.h


        F2C_INT64_FLAGS="-DAllow_TYQUAD"
        TYPE_INT64=${typName}
        ;;
    esac
    fi
done

rm -rf ./szchk ./szchk.*

# --------------------------------------------------------------------
# check size of integer enough to hold size of void *

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking integer type enough to hold void pointer" >&5
$as_echo_n "checking integer type enough to hold void pointer... " >&6; }
${CC} -I. ${CPPFLAGS} ${CFLAGS} ${hasLLFlags} ./buildutils/chkvoidp.c -o chkvoidp
voidPint="unknown"
if test -x ./chkvoidp; then
    voidPint=`./chkvoidp`
else
    as_fn_error $? "can't create check program." "$LINENO" 5
fi
if test "x${voidPint}" = "xunknown"; then
    as_fn_error $? "can't determine????" "$LINENO" 5
else
