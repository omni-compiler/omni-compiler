    fi

    # check -lgmp
    echo "int main(){return 0;}" > $tmp_file_c
    ${CC} -L$GMP_LIB_DIR -lgmp $tmp_file_c -o $tmp_file
    if test $? -ne 0; then
	rm -f $tmp_file $tmp_file_c
	as_fn_error $? "-lgmp not found. Please attach --with-gmp=<DIR> or --with-gmp-lib=<DIR>." "$LINENO" 5
    fi
    rm -f $tmp_file $tmp_file_c




    #### MPFR ####
    # check dirs
    if ! test -d "$MPFR_INCLUDE_DIR"; then
	as_fn_error $? "$MPFR_INCLUDE_DIR not found.  Please attach --with-mpfr=<DIR> or --with-mpfr-include=<DIR>." "$LINENO" 5
    fi

    if ! test -d "$MPFR_LIB_DIR"; then
	MPFR_LIB_DIR=${MPFR_PREFIX}/lib
	if ! test -d "$MPFR_LIB_DIR"; then
	    as_fn_error $? "$MPFR_LIB_DIR not found.  Please attach --with-mpfr=<DIR> or --with-mpfr-lib=<DIR>." "$LINENO" 5
	fi
    fi

    # check mpfr.h
    echo "#include <mpfr.h>" > $tmp_file_c
    ${CC} -E -I$MPFR_INCLUDE_DIR -I$GMP_INCLUDE_DIR $tmp_file_c 2> /dev/null > /dev/null
    if test $? -ne 0; then
	rm -f $tmp_file_c
	as_fn_error $? "mpfr.h not found. Please attach --with-mpfr=<DIR> or --with-mpfr-include=<DIR>." "$LINENO" 5
    fi

    # check -lmpfr
    echo "int main(){return 0;}" > $tmp_file_c
    ${CC} -L$MPFR_LIB_DIR -lmpfr $tmp_file_c -o $tmp_file
    if test $? -ne 0; then
	rm -f $tmp_file tmp_file_c
	as_fn_error $? "-lmpfr not found. Please attach --with-mpfr=<DIR> or --with-mpfr-lib=<DIR>." "$LINENO" 5
    fi
    rm -f $tmp_file tmp_file_c



fi

