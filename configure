# libgmp check
omniLimbT="int"
omniExpT="int"
omniQRealPrecInLimb="1"
omniQRealFillGap=""
if test "x${doQReal}" = "xyes"; then
    omniQPrec=$qRealPrecBits
    $as_echo "#define ENABLE_QREAL 1" >>confdefs.h


    cat >>confdefs.h <<_ACEOF
#define OMNI_QREAL_PREC $omniQPrec
_ACEOF



    qcCC () {
    rm -f $2 ${2}.*
    ${CC} -I. ${CPPFLAGS} ${CFLAGS} -DENABLE_QREAL ${hasLLFlags} ${gmpIncOpt} $3 $1 -o $2 ${gmpLibOpt}
    if test -x $2; then
        ret=`$2 $4`
        if test "x${ret}" = "x"; then
            as_fn_error $? "can't determine??." "$LINENO" 5
        else
            rm -f $2 ${2}.*
            echo ${ret}
        fi
    else
        as_fn_error $? "can't create check program." "$LINENO" 5
    fi
    }

    # check size of mp_exp_t
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking size of GNU-MP mp_exp_t" >&5
$as_echo_n "checking size of GNU-MP mp_exp_t... " >&6; }
    eSz=`qcCC buildutils/csMPexp.c ./csMPexp`
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${eSz}" >&5
$as_echo "${eSz}" >&6; }
    omniExpT=""
    case ${eSz} in
    2)
        omniExpT=${TYPE_INT16};;
    4)
        omniExpT=${TYPE_INT32};;
    8)
        omniExpT=${TYPE_INT64};;
    *)
        as_fn_error $? "can't determine basic type of mp_exp_t ??." "$LINENO" 5
    esac
    cat >>confdefs.h <<_ACEOF
#define OMNI_SIZEOF_QEXP_T $eSz
_ACEOF



    # check size of mp_limb_t
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking size of GNU-MP mp_limb_t" >&5
$as_echo_n "checking size of GNU-MP mp_limb_t... " >&6; }
    lSz=`qcCC buildutils/csMPlimb.c ./csMPlimb`
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${lSz}" >&5
$as_echo "${lSz}" >&6; }
    omniLimbT=""
    case ${lSz} in
    2)
        omniLimbT=${TYPE_INT16};;
    4)
        omniLimbT=${TYPE_INT32};;
    8)
        omniLimbT=${TYPE_INT64};;
    *)
        as_fn_error $? "can't determine basic type of mp_limb_t ??." "$LINENO" 5;;
    esac
    cat >>confdefs.h <<_ACEOF
#define OMNI_SIZEOF_QLIM_T $lSz
_ACEOF



    # check how many mp_limb_t the GNU-MP needed for N bit precision.
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking how many mp_limb_t needed for $omniQPrec bit precision" >&5
$as_echo_n "checking how many mp_limb_t needed for $omniQPrec bit precision... " >&6; }
    omniQRealPrecInLimb=`qcCC buildutils/cMPlb.c ./cMPlb '' $omniQPrec`
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: $omniQRealPrecInLimb" >&5
$as_echo "$omniQRealPrecInLimb" >&6; }
    cat >>confdefs.h <<_ACEOF
#define OMNI_QLIM_LEN $omniQRealPrecInLimb
_ACEOF



    # check _omQReal_t needs a gap between _mp_exp and _mp_d[omniQRealPrecInLimb].
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking _omQReal_t needs a gap between _mp_exp and _mp_d" >&5
$as_echo_n "checking _omQReal_t needs a gap between _mp_exp and _mp_d... " >&6; }
    gapInByte=`expr $lSz - $eSz`
    subStr=""
    if test ${gapInByte} -gt 0; then
        case ${gapInByte} in
        2)
            # lim == int, exp == short
            subStr="${TYPE_INT16} _gap_;";;
        4)
            # lim == long long, exp == int
            subStr="${TYPE_INT32} _gap_;";;
        *)
            # lim == long long, exp == short

            subStr="char _gap_[${gapInByte}];"

            ;;
        esac
    fi
    if test "x${subStr}" != "x"; then
        #TODO
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes, put a \"${subStr}\" member in _omQReal_t." >&5
$as_echo "yes, put a \"${subStr}\" member in _omQReal_t." >&6; }
        omniQRealFillGap=${subStr}
        $as_echo "#define OMNI_QREAL_NEED_GAP_MEMBER 1" >>confdefs.h


        cat >>confdefs.h <<_ACEOF
#define OMNI_QREAL_GAP_SIZE ${gapInByte}
_ACEOF


    else
        omniQRealFillGap="/* if ((sizeof(mp_exp_t) < sizeof(mp_limb_t)), here must be a gap filling member. */"
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    fi

    # check libgmp has an mpf_floor(), appeared in gmp3 later.
    oLIBS=${LIBS}
    LIBS="${LIBS} ${gmpLibOpt}"
    oCFLAGS=${CFLAGS}
    CFLAGS="${CFLAGS} ${CPPFLAGS} -DENABLE_QREAL ${gmpIncOpt}"
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking mpf_floor() in $LIBS" >&5
$as_echo_n "checking mpf_floor() in $LIBS... " >&6; }
    hasFloor=no
    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

        #include "gmp.h"

int
main ()
{

        mpf_t a;
        mpf_t b;
        (void)mpf_floor(a, b);

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }; hasFloor=yes
